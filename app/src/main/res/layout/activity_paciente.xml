<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Presenter.PacienteActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="168dp"
        android:layout_y="18dp"
        android:text="paciente" />

    <!--Botao Logout Paciente-->
    <Button
        android:id="@+id/buttonLogoutPaciente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:layout_x="21dp"
        android:layout_y="448dp"
        android:text="@string/logout" />

    <!--Botao Editar Paciente-->
    <Button
        android:id="@+id/buttonEditarPaciente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:layout_x="48dp"
        android:layout_y="110dp"
        android:text="@string/editar" />

    <!--Botao Informações Medicas-->
    <Button
        android:id="@+id/buttonInformacoesMedicas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:layout_x="177dp"
        android:layout_y="112dp"
        android:text="@string/informacoesMedicas" />


</AbsoluteLayout>