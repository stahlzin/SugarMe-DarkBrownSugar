<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">

    <GridLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:columnCount="2"
        android:layout_weight="1"
        android:rowCount="9"
        android:useDefaultMargins="true">

        <TextView
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:gravity="center"
            android:textAppearance="@android:style/TextAppearance.Medium"
            android:textStyle="bold"
            android:layout_column="0"
            android:layout_row="0"
            android:text="@string/DataRegistroInter" />

        <TextView
            android:id="@+id/dataInterTextView"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_row="1"
            android:layout_column="0"
            android:textAppearance="@android:style/TextAppearance.Medium"
            android:text="TextView" />


        <TextView
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:gravity="center"
            android:textAppearance="@android:style/TextAppearance.Medium"
            android:textStyle="bold"
            android:layout_column="0"
            android:layout_row="2"
            android:text="@string/horaRegistroInter" />

        <TextView
            android:id="@+id/horaInterTextView"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_column="0"
            android:layout_row="3"
            android:textAppearance="@android:style/TextAppearance.Medium"
            android:text="TextView" />

        <TextView
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:gravity="center"
            android:textAppearance="@android:style/TextAppearance.Medium"
            android:textStyle="bold"
            android:layout_column="0"
            android:layout_row="4"
            android:text=" " />

        <TextView
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:gravity="center"
            android:textAppearance="@android:style/TextAppearance.Medium"
            android:textStyle="bold"
            android:layout_column="0"
            android:layout_row="5"
            android:text=" " />

        <TextView
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:gravity="center"
            android:textAppearance="@android:style/TextAppearance.Medium"
            android:textStyle="bold"
            android:layout_column="0"
            android:layout_row="6"
            android:text=" " />

        <TextView
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:gravity="center"
            android:textAppearance="@android:style/TextAppearance.Medium"
            android:textStyle="bold"
            android:layout_column="0"
            android:layout_row="7"
            android:text="@string/anotacoesInter" />

        <TextView
            android:id="@+id/anotacoesInterTextView"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_columnSpan="2"
            android:layout_column="0"
            android:layout_row="8"
            android:text="@string/anotacoesInter" />



        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="1"
            android:layout_row="0"
            android:textStyle="bold"
            android:layout_columnWeight="1"
            android:textAppearance="@android:style/TextAppearance.Medium"
            android:text="@string/sintomasInter" />

        <TextView
            android:id="@+id/hiperInterTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_columnWeight="1"
            android:background="@color/colorHiper"
            android:layout_row="1"
            android:layout_column="1"
            android:textAppearance="@android:style/TextAppearance.Medium"
            android:text="TextView" />

        <TextView
            android:id="@+id/hipoInterTextView"
            android:background="@color/colorHipo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_columnWeight="1"
            android:layout_column="1"
            android:layout_row="2"
            android:textAppearance="@android:style/TextAppearance.Medium"
            android:text="TextView" />

        <TextView
            android:id="@+id/sedeInterTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="1"
            android:layout_row="3"
            android:layout_columnWeight="1"
            android:background="@color/colorNormal"
            android:textAppearance="@android:style/TextAppearance.Medium"
            android:text="@string/sintomasInter" />

        <TextView
            android:id="@+id/nauseaInterTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_columnWeight="1"
            android:background="@color/colorAccentOver"
            android:layout_row="4"
            android:textAppearance="@android:style/TextAppearance.Medium"
            android:layout_column="1"
            android:text="TextView" />

        <TextView
            android:id="@+id/desmaioInterTextView"
            android:background="@color/colorDesmaio"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_columnWeight="1"
            android:layout_column="1"
            android:layout_row="5"
            android:textAppearance="@android:style/TextAppearance.Medium"
            android:text="TextView" />

        <TextView
            android:id="@+id/internacaoInterTextView"
            android:background="@color/colorInternacao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_columnWeight="1"
            android:layout_column="1"
            android:layout_row="6"
            android:textAppearance="@android:style/TextAppearance.Medium"
            android:text="TextView" />

    </GridLayout>
</LinearLayout>